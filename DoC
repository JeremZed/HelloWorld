abstract Personnage
  - nom               : string
  - droitier          : bool
  - gaucher           : bool
  - experience        : int
  - sexe              : Sexe
  - apparence         : Apparence
  - race              : Race
  - classe            : Classe
  - equipement        : Stuff
  - caracteristique   : Caracteristiques
  - getNom()
  - setNom(str)
  - getDroitier()
  - setDroitier(bool)
  - getGaucher()
  - setGaucher(bool)
  - getExperience()
  - setExperience(int)
  - getSexe()
  - setSexe(Sexe)
  - getApparence()
  - setApparence(Apparence)
  - getRace()
  - setRace(Race)
  - getClasse()
  - setClasse(Classe)
  - getNiveau()
  - getCalculNiveau()
  - getEquipement()
  - setEquipement(Stuff)
  - getCaracteristique()
  - setCaracteristique(Caracteristiques)
  
Player extends Personnage
  - __construct() > Personnage.setNom(str)  
